# Copy this file to `.env` before starting the stack:
#   cp .env.example .env

# Shared container settings
PUID=1000
PGID=1000
TZ=Europe/Amsterdam

# Plex
# Example: http://192.168.1.10:32400/
PLEX_ADVERTISE_IP=http://192.168.1.10:32400/

# SABnzbd
# Comma-separated host allowlist for SAB web/API access.
SABNZBD_HOST_WHITELIST=localhost,127.0.0.1,wireguard,192.168.1.10,192.168.1.*
